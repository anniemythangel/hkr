{
  "name": "@hooker/shared",
  "version": "0.0.1",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --dts --format esm,cjs --out-dir dist",
    "dev": "tsup src/index.ts --dts --watch",
    "test": "vitest run || true"
  },
  "devDependencies": {
    "tsup": "^7.3.0",
    "typescript": "^5.4.0"
  }
}
